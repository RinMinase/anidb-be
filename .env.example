MAIL_MAILER=smtp
MAIL_HOST=mailhog
MAIL_PORT=1025
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS=null
MAIL_FROM_NAME="${APP_NAME}"

FIRE_PROJECT_ID=project_id
FIRE_KEY="NOTE: change all backslash 'n' to \\n"
FIRE_EMAIL=project_email
FIRE_CLIENT_ID=000

SCRAPER_BASE_URI=example.com
RELEASE_BASE_URI=api.example.com

MAILGUN_API_KEY=sample-sample-sample
