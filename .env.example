APP_PORT=80

DISABLE_DB=false
DISABLE_FIREBASE=false
DISABLE_SCRAPER=false
DISABLE_MAILGUN=false
DISABLE_MYSQL=false

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
