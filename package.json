{
  "name": "anidb-be",
  "description": "Rin Minase's AniDB API Service utilizing Laravel deployed to Heroku",
  "version": "0.1.0",
  "license": "MIT",
  "author": "Rin Minase",
  "repository": {
    "type": "git",
    "url": "https://github.com/RinMinase/anidb-be.git"
  },
  "scripts": {
    "docs": "apidoc -i app/Controllers -o public/docs",
    "build": "npm run docs",
    "cz": "cz"
  },
  "dependencies": {
    "apidoc": "^0.53.0",
    "commitizen": "^4.2.5",
    "cz-emoji-conventional": "^1.0.1",
    "devmoji": "^2.3.0",
    "yorkie": "^2.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "apidoc": {
    "name": "Rin's AniDB API / Middleware",
    "version": "0.1.0",
    "title": "AniDB API Documentation",
    "url": "https://anidb-staging.herokuapp.com/docs",
    "order": [
      "✅ Success ✅",
      "❌ Errors ❌"
    ]
  },
  "gitHooks": {
    "prepare-commit-msg": "devmoji -e"
  },
  "config": {
    "commitizen": {
      "path": "cz-emoji-conventional"
    }
  }
}
